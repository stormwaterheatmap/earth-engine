/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var image = ee.Image("NASA/ASTER_GED/AG100_003"),
    image2 = ee.Image("USGS/NED");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var ndvi = image.select('ndvi')
Map.addLayer(ndvi)
function uSnic(image, size, compactness) {
var  userSnic = ee.Algorithms.Image.Segmentation.SNIC(
    {image: image,
    size: size,
    compactness: compactness});
    return userSnic
}
var ss = uSnic(ndvi, 50, 0)
var el = uSnic(image2, 35, 5)
Map.addLayer(ss.select('ndvi_mean'))//elect('clusters'))
Map.addLayer(el)
var sq = uSnic(ee.Image(1),20,99)
Map.addLayer(sq)
